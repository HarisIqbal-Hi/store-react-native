{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["dom", "es2017"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "types.d.ts",
    "types/**/*",
    "constants/constantsdec.d.ts"
  ]
}
